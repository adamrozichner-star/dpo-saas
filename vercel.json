{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["fra1"],
  "crons": [
    {
      "path": "/api/billing/recurring",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/email-sequence",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/monthly-digest",
      "schedule": "0 9 1 * *"
    }
  ]
}
